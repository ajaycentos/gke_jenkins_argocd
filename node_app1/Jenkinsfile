pipeline {
  agent {
        label 'agent-nodes'
    }
  stages 
  {
    stage('Docker Build') 
    {
      agent {
		label 'agent-nodes'
	    }
      steps {
            checkout scm
            sh '''
              cd node_app1
              ls -l
              docker build -t ajay/sampleapp:v1 .
               '''
            }
    }
  }
}
